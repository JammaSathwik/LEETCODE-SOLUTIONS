class Solution 
{
public:
    vector<string> fizzBuzz(int n) 
    {
        vector<string> x;
        for(int i=1;i<=n;i++)
        {
            if(i%3 && i%5) x.push_back(to_string(i));
            else if(i%3) x.push_back("Buzz");
            else if(i%5) x.push_back("Fizz");
            else x.push_back("FizzBuzz");
        }
        return x;
    }
};
